<%= simple_form_for(@research) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :type %>
    <%= f.input :question %>
    <%= f.input :subquestion %>
    <%= f.input :data %>
    <%= f.input :range %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
